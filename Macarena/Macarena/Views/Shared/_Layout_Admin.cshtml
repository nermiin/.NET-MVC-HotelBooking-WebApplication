@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>@ViewBag.Title Sufee Admin - HTML5 Admin Template</title>
	<meta name="description" content="Sufee Admin - HTML5 Admin Template">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<link rel="apple-touch-icon" href="~/Admin_Paneli/apple-icon.png">
	<link rel="shortcut icon" href="~/Admin_Paneli/favicon.ico">

	@Styles.Render("~/Content/bootstrap.min.css");
	@Styles.Render("~/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css");
	@Styles.Render("~/Admin_Paneli/vendors/themify-icons/css/themify-icons.css");
	@Styles.Render("~/Admin_Paneli/vendors/flag-icon-css/css/flag-icon.min.css");
	@Styles.Render("~/Admin_Paneli/vendors/selectFX/css/cs-skin-elastic.css");
	@Styles.Render("~/Admin_Paneli/vendors/jqvmap/dist/jqvmap.min.css");
	@Styles.Render("~/Admin_Paneli/assets/css/style.css");
	@Styles.Render("https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800");

</head>
<body><div class="container-fluid fixed-top position-fixed">
	@Html.Partial("_Header_Admin")

	@RenderBody()
</div>
	


	@Scripts.Render("~/Admin_Paneli/vendors/jquery/dist/jquery.min.js");
	@Scripts.Render("~/Admin_Paneli/vendors/popper.js/dist/umd/popper.min.js");
	@Scripts.Render("~/Admin_Paneli/vendors/bootstrap/dist/js/bootstrap.min.js");
	@Scripts.Render("~/Admin_Paneli/assets/js/main.js");
	@Scripts.Render("~/Admin_Paneli/vendors/chart.js/dist/Chart.bundle.min.js");
	@Scripts.Render("~/Admin_Paneli/assets/js/dashboard.js");
	@Scripts.Render("~/Admin_Paneli/assets/js/widgets.js");
	@Scripts.Render("~/Admin_Paneli/vendors/jqvmap/dist/jquery.vmap.min.js");
	@Scripts.Render("~/Admin_Paneli/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js");
	@Scripts.Render("~/Admin_Paneli/vendors/jqvmap/dist/maps/jquery.vmap.world.js");
	<script>
		(function ($) {
			"use strict";

			jQuery('#vmap').vectorMap({
				map: 'world_en',
				backgroundColor: null,
				color: '#ffffff',
				hoverOpacity: 0.7,
				selectedColor: '#1de9b6',
				enableZoom: true,
				showTooltip: true,
				values: sample_data,
				scaleColors: ['#1de9b6', '#03a9f5'],
				normalizeFunction: 'polynomial'
			});
		})(jQuery);</script>
</body>
</html>
