@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewBag.Title - Macarena </title>
	@Styles.Render("~/Content/bootstrap.min.css");
	@Styles.Render("~/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css");
	@Styles.Render("~/plugins/OwlCarousel2-2.2.1/owl.carousel.css");
	@Styles.Render("~/plugins/OwlCarousel2-2.2.1/owl.theme.default.css");
	@Styles.Render("~/plugins/OwlCarousel2-2.2.1/animate.css");
	@Styles.Render("~/plugins/js-flickr-gallery-1.24/js-flickr-gallery.css");
	@Styles.Render("~/Content/css/main_styles.css");
	@Styles.Render("~/Content/css/responsive.css");
	@*@Styles.Render("~/Content/css/styles/blog_styles.css");
	@Styles.Render("~/Content/css/styles/blog_responsive.css");*@
	@Styles.Render("~/Content/css/Stillerim.css");
</head>
<body>
	@Html.Partial("_Header_Account")
	<div class="container">
		@RenderBody()
	</div>
	@Html.Partial("_Footer")

	@Scripts.Render("~/Scripts/js/jquery-3.2.1.min.js");
	@Scripts.Render("~/Scripts/umd/popper.js");
	@Scripts.Render("~/Scripts/bootstrap.min.js");
	@Scripts.Render("~/plugins/greensock/TweenMax.min.js");
	@Scripts.Render("~/plugins/greensock/TimelineMax.min.js");
	@Scripts.Render("~/plugins/scrollmagic/ScrollMagic.min.js");
	@Scripts.Render("~/plugins/greensock/animation.gsap.min.js");
	@Scripts.Render("~/plugins/greensock/ScrollToPlugin.min.js");
	@Scripts.Render("~/plugins/progressbar/progressbar.min.js");
	@Scripts.Render("~/plugins/OwlCarousel2-2.2.1/owl.carousel.js");
	@Scripts.Render("~/plugins/easing/easing.js");
	@Scripts.Render("~/Scripts/js/elements_custom.js");
	@Scripts.Render("~/Scripts/js/custom.js");
	@Scripts.Render("~/Scripts/js/Slider.js");
	@Scripts.Render("https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js");
	@Scripts.Render("~/Scripts/js/kodum.js");
</body>
</html>
